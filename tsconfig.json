{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "outDir": "wwwroot/js",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es6",
    "esModuleInterop": true,
    "moduleResolution": "Node"
  },
    "include": [
      "App/index.tsx"
    ]
  }